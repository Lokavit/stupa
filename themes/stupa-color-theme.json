{
  "name": "stupa",
  "type": "dark",
  "colors": {
    "activityBar.background": "#33403e",
    "activityBarBadge.background": "#63a698",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#63a698",
    "editor.background": "#1f2625",
    "progressBar.background": "#fd4b00",
    "titleBar.activeBackground": "#33403e",
    "sideBar.background": "#293331",
    "sideBarSectionHeader.background": "#293331",
    "dropdown.background": "#1d2625",
    "menu.background": "#1d2625",
    "list.activeSelectionBackground": "#33403e",
    "list.inactiveSelectionBackground": "#33403e",
    "list.dropBackground": "#475957",
    "list.hoverBackground": "#33403e",
    "list.focusBackground": "#475957",
    "statusBar.background": "#33403e",
    "statusBar.noFolderBackground": "#009578",
    "statusBar.debuggingBackground": "#009578",
    "editorGroupHeader.tabsBackground": "#1f2625",
    "tab.activeBackground": "#33403e",
    "tab.activeBorder": "#00ffff",
    "tab.inactiveBackground": "#1f2625",
    "button.background": "#009578",
    "editorWidget.background": "#33403e",
    "input.background": "#1f2625",
    "input.border": "#33403e",
    "selection.background": "#63a69844",
    "editor.wordHighlightStrongBackground": "#63a69844",
    "focusBorder": "#63a698",
    "editor.lineHighlightBackground": "#262e2d",
    "editor.selectionBackground": "#63a69844",
    "pickerGroup.foreground": "#cae0dc",
    "textLink.foreground": "#cae0dc",
    "editorSuggestWidget.highlightForeground": "#63a698"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7f9993"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "meta.object-literal.key.js"
      ],
      "settings": {
        "foreground": "#b3f5f5"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#efebeb"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#fd5d78"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#44efce"
      }
    },
    {
      "name": "JSDoc Block",
      "scope": ["punctuation.definition.block.tag.jsdoc"],
      "settings": {
        "foreground": "#44efce"
      }
    },
    {
      "name": "Double Quote",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#fff7b1"
      }
    },
    {
      "name": "Curly Brace",
      "scope": [
        "punctuation.definition.bracket.curly.begin.jsdoc",
        "punctuation.definition.bracket.curly.end.jsdoc"
      ],
      "settings": {
        "foreground": "#fff7b1"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        // "foreground": "#44efce"
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#2ECBAC"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#99b9ff"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#5da8fd"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#fff7b1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#fff7b1"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#fd5d78"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8fccba"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#99b9ff"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#99b9ff"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fff17d"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#99b9ff"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#99b9ff"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fd5d78"
      }
    },
    {
      "name": "Type",
      "scope": "storage.type.primitive, support.type, entity.name.type, keyword.type, keyword.other.unit",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "String",
      "scope": "string, meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#63a698"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#29d5b0"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, support.constant",
      "settings": {
        "foreground": "#55cfb5"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#63a698"
      }
    },
    {
      "name": "Undefined",
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#63a698"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#36b7f8"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super, keyword.other.get, keyword.other.set",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Constants",
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#57d5ff"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class entity.name.type.module",
      "settings": {
        "foreground": "#57ffab",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this, variable.language.special.self",
      "settings": {
        "foreground": "#94ff6a"
      }
    },
    {
      "name": "Storage Modifier [e.g. class, extends, static] (JS",
      "scope": "storage.modifier, storage.type.class.js, variable.language.this, variable.language.this.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#57d8ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#a3ff57"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      // md文件 标题 # 标识符后面的文字
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#77e0e7"
      }
    },
    {
      // md 文件 列表 - 的颜色
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#03c7f8"
      }
    },

    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference, markup.underline.link.image",
      "settings": {
        "foreground": "#62c3bd"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Markdown code",
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#6aabff"
      }
    },
    {
      "name": "Markdown italic", // 斜体字
      "scope": "markup.italic",
      "settings": {
        "foreground": "#69e0c6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#57cdff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#57fffc",
        "fontStyle": "italic bold"
      }
    }
  ]
}
